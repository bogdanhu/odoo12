<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>
    
        <record id="view_account_barcode_form_inherit" model="ir.ui.view">
        <field name="name">account.order.form.account.barcode</field>
        <field name="model">account.invoice</field>
        <field name="inherit_id" ref="account.invoice_form"/>
        <field name="arch" type="xml">
            <xpath expr="//page[@name='other_info']/group/group/field[@name='name']" position="after">
                <field name="print_barcode"/>
            </xpath>
        </field>
        </record>

    </data>
</odoo>